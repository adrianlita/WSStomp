<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net472</TargetFramework>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Title>WSStomp</Title>
        <Authors>Adrian Lita</Authors>
        <Description>STOMP over WebSockets in C#.</Description>
        <Copyright>AXIPlus</Copyright>
        <PackageProjectUrl>https://github.com/adrianlita/WSStomp</PackageProjectUrl>
	<PackageLicenseExpression>MIT</PackageLicenseExpression>
        <RepositoryUrl>https://github.com/adrianlita/WSStomp</RepositoryUrl>
	<PackageReadmeFile>README.md</PackageReadmeFile>
        <RepositoryType>git</RepositoryType>
        <PackageTags>stomp websockets async protocol</PackageTags>
        <Company>AXIPlus</Company>
        <AssemblyVersion>1.1.0.0</AssemblyVersion>
        <FileVersion>1.1.0.0</FileVersion>
        <Version>1.1.0</Version>
        <RootNamespace>AXIPlus</RootNamespace>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="JetBrains.Annotations" Version="2024.3.0" />
	<None Include="README.md" Pack="true" PackagePath="\"/>
    </ItemGroup>

</Project>
